# WalletConnect Configuration
# Get your project ID from https://cloud.walletconnect.com
VITE_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id_here

# Application Configuration
VITE_APP_NAME=ZetaLend
VITE_APP_DESCRIPTION=Supply collateral and borrow assets across EVM chains

# Network Configuration
# Set to 'true' to force testnet mode even in production builds
# By default, testnets are used in development mode and mainnets in production
VITE_ENABLE_TESTNETS=true

# Default chain ID to connect to (optional)
# VITE_DEFAULT_CHAIN_ID=7001


# ALCHEMY_API_KEY
VITE_ALCHEMY_API_KEY=